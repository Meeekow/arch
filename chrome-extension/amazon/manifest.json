{
  "manifest_version": 3,
  "name": "Move Tab",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "windows"
  ],
  "action": {},
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.amazon.com/s?k*"],
      "js": ["content.js"]
    }
  ]
}
