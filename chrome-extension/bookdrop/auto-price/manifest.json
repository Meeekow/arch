{
  "manifest_version": 3,
  "name": "Auto Price",
  "version": "1.1",
  "description": "Set price to 49.99",
  "permissions": [
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {},
  "incognito": "not_allowed",
  "content_scripts": [
    {
      "matches": [
        "https://bookparse.com/dashboard/2/ebay-listing"
      ],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
